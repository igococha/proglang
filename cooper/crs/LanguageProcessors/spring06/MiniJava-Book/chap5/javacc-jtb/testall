#!/bin/shecho "Testing"for file in /home/lookout/f/vids/cs352/Tests/MiniJava/*.javado   echo ".\c"   java Typecheck $file > tmp   grep TE $file > /dev/null     if [ $? -eq 0 ]	then	    diff tmp err >/dev/null 	    if [ $? -ne 0 ]		then		    echo		    echo "$file  failed test [TE]"		  		fi		   else	diff tmp noerr > /dev/null	    if [ $? -ne 0 ]		then		   echo		   echo "$file  failed test [NE]"		   		fi		    fidone